<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 6.2.0//EN"
"http://www.liferay.com/dtd/liferay-friendly-url-routes_6_2_0.dtd">

<routes>
    <route>
        <pattern>/{instanceId}/call</pattern>
        <implicit-parameter name="p_p_lifecycle">0</implicit-parameter>
        <implicit-parameter name="mvcPath">/html/callmanagement/view.jsp</implicit-parameter>
    </route>
    <route>
        <pattern>/{instanceId}/call</pattern>
        <implicit-parameter name="p_p_lifecycle">1</implicit-parameter>
        <implicit-parameter name="mvcPath">/html/callmanagement/view.jsp</implicit-parameter>
    </route>
    <route>
        <pattern>/{instanceId}/add</pattern>
        <implicit-parameter name="p_p_lifecycle">0</implicit-parameter>
        <implicit-parameter name="mvcPath">/html/callmanagement/add_call.jsp</implicit-parameter>
    </route>
    <route>
        <pattern>/{instanceId}/{callId}/view</pattern>
        <implicit-parameter name="p_p_lifecycle">0</implicit-parameter>
        <implicit-parameter name="mvcPath">/html/callmanagement/asset/full_content.jsp</implicit-parameter>
    </route>
    <route>
        <pattern>/{instanceId}/{callId}/view</pattern>
        <ignored-parameter name="javax.portlet.action"/>
        <implicit-parameter name="p_p_lifecycle">1</implicit-parameter>
        <implicit-parameter name="mvcPath">/html/callmanagement/asset/full_content.jsp</implicit-parameter>
    </route>
    <route>
        <pattern>/{instanceId}/{callId}/edit</pattern>
        <implicit-parameter name="p_p_lifecycle">0</implicit-parameter>
        <implicit-parameter name="mvcPath">/html/callmanagement/edit_call.jsp</implicit-parameter>
    </route>
</routes>